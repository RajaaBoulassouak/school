<%= form_for [@student, @address] do |f| %>
  <p>
    <% f.label :description %>
    <%= f.text_area :description %>
  </p>
  <p>
    <% f.label :street %>
    <%= f.text_area :street %>
  </p>
  <p>
    <% f.label :city %>
    <%= f.text_area :city %>
  </p>
  <p>
    <% f.label :zip_code %>
    <%= f.text_area :zip_code %>
  </p>
  <p>
    <%= f.submit 'Submit' %>
  </p>
<% end %>